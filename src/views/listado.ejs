<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
     <link rel="stylesheet" href="/css/styles.css">
    <title>TurnosDH</title>
  </head>
  
    <%- include('./partials/header.ejs')%>


    <div>
<h1>Lista de Turnos</h1>
  </div>
   <div class="boxlist">
    <% for( let i = 0; i < caja.length; i++ ) { %>
                    <ul class="list-group">                    
                            <li class="list-group-item"><%= caja[i].box %>
                            <%= caja[i].code %></li>
                            <button class="button-edit">editar</button>
                    </ul>                                           
                <% } %>
   </div> 
 <body>
<h2>Crear Turno</h2>
<form class="boxlist" action="listado" method="POST" enctype="application/x-www-form-urlencoded">
 <div class="list-group"> 
  <label>BOX</label>
 <input type="text" name="box" data-nombre="box">
</div>
 <div class="list-group">
  <label>CODIGO</label>
 <input type="text" name="code" data-nombre="code">
 </div>
 
 <button class="button-edit" type="submit">Insertar</button>
</form>


</div>
  
  

  </body>
</html>


